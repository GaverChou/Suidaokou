<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gaverchou.suidaokou">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:name=".TunnelApplication"
        android:theme="@style/AppTheme.Toolbar">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ProfileActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.Toolbar.TransparentActivity"></activity>
        <activity
            android:name=".ActionInfoActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.Toolbar.TransparentActivity"></activity>
        <activity
            android:name=".WangoDetail"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.Toolbar"></activity>
        <activity
            android:name=".GroupActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.Toolbar"></activity>
        <activity
            android:name=".InformDetailActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.Toolbar"></activity>
        <activity
            android:name=".ConsulationDetailActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.Toolbar"></activity>
    </application>

</manifest>
